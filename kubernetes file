apiVersion: apps/v1
kind: Deployment
metadata:
  name: gcp-demo-app
spec:
  replicas: 2
  selector:
    matchLabels:
      app: demo
  template:
    metadata:
      labels:
        app: demo
    spec:
      containers:
      - name: demo
        image: gcr.io/my-project/my-app
        ports:
        - containerPort: 8080
